{
  "name": "expotemplate",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "android:staging": "dotenv -e .env.staging -- expo run:android",
    "ios:staging": "dotenv -e .env.staging -- expo run:ios",
    "start:staging": "dotenv -e .env.staging -- expo start",
    "android:development": "dotenv -e .env.development -- expo run:android",
    "ios:development": "dotenv -e .env.development -- expo run:ios",
    "start:development": "dotenv -e .env.development -- expo start",
    "android:production": "dotenv -e .env.production -- expo run:android",
    "ios:production": "dotenv -e .env.production -- expo run:ios",
    "start:production": "dotenv -e .env.production -- expo start",
    "build:android:staging": "dotenv -e .env.staging -- sh -c 'expo prebuild --platform android && cd android && ./gradlew assembleRelease'",
    "build:android:development": "dotenv -e .env.development -- sh -c 'expo prebuild --platform android && cd android && ./gradlew assembleRelease'",
    "build:android:production": "dotenv -e .env.production -- sh -c 'expo prebuild --platform android && cd android && ./gradlew assembleRelease'",
    "build:ios:staging": "dotenv -e .env.staging -- expo prebuild --platform ios && cd ios && open ExpoTemplate.xcworkspace",
    "build:ios:development": "dotenv -e .env.development -- expo prebuild --platform ios && cd ios && open ExpoTemplate.xcworkspace",
    "build:ios:production": "dotenv -e .env.production -- expo prebuild --platform ios && cd ios && open ExpoTemplate.xcworkspace",
    "postinstall": "expo prebuild && patch-package"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/drawer": "^7.5.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@reduxjs/toolkit": "^2.2.3",
    "@types/react": "~19.0.10",
    "axios": "^1.6.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv-cli": "^8.0.0",
    "expo": "53.0.22",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-localization": "~16.1.6",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "i18n-js": "^4.4.3",
    "patch-package": "^8.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-mmkv": "^3.3.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-nitro-modules": "^0.28.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.0",
    "react-native-unistyles": "^3.0.10",
    "react-native-web": "^0.20.0",
    "react-redux": "^9.1.1",
    "redux-persist": "^6.0.0",
    "typescript": "~5.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
