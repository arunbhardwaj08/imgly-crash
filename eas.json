{
  "cli": {
    "version": ">= 7.8.2",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "distribution": "internal",
      "node": "20.18.0",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://base-url/api/v1/",
        "EXPO_PUBLIC_SOCKET_URL": "https://socket-url.tech"
      }
    },
    "staging": {
      "distribution": "internal",
      "node": "20.18.0",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://base-url/api/v1/",
        "EXPO_PUBLIC_SOCKET_URL": "https://socket-url.tech"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "20.18.0",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://base-url/api/v1/",
        "EXPO_PUBLIC_SOCKET_URL": "https://socket-url.tech"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
